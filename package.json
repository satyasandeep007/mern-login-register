    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "heroku": "^7.35.1",
    "md5": "^2.2.1",
    "mongoose": "^5.8.3",
    "mongoose-unique-validator": "^2.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
